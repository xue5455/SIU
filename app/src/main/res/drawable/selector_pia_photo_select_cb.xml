<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:state_enabled="false">
        <bitmap android:src="@mipmap/ic_checkbox_disabled" />
    </item>
    <item android:state_checked="false" android:state_enabled="true">
        <bitmap android:src="@mipmap/ic_checkbox_nor" />
    </item>
    <item android:state_checked="true">
        <bitmap android:src="@mipmap/ic_checkbox_selected" />
    </item>
</selector>